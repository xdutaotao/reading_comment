#!/usr/bin/env ktap

var count1 = 0
trace kmem:kmalloc {
	count1 += 1
}

var count2 = 0
trace kmem:kfree {
	count2 += 1
}

var count3 = 0
trace kmem:mm_page_alloc {
	count3 += 1
}

var count4 = 0
trace kmem:mm_page_free {
	count4 += 1
}

trace_end {
	print("\n")
	print("kmem:kmalloc:\t", count1)
	print("kmem:kfree:\t", count2)
	print("kmem:mm_page_alloc:", count3)
	print("kmem:mm_page_free:", count4)
}
